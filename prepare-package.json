{
  "name": "quimbi-macos-preparation-tool",
  "version": "1.0.0",
  "description": "Standalone tool to prepare macOS systems for Quimbi Support Assistant",
  "main": "prepare-macos.js",
  "bin": {
    "prepare-quimbi": "./prepare-macos.js"
  },
  "scripts": {
    "build": "pkg prepare-macos.js --target node18-macos-arm64 --output dist/prepare-quimbi-macos",
    "build-universal": "pkg prepare-macos.js --target node18-macos-x64,node18-macos-arm64 --output dist/prepare-quimbi-macos",
    "test": "node prepare-macos.js --help"
  },
  "keywords": [
    "ollama",
    "macos",
    "installer",
    "quimbi",
    "ai",
    "preparation"
  ],
  "author": "Quimbi AI Team",
  "license": "MIT",
  "pkg": {
    "scripts": "prepare-macos.js",
    "targets": [
      "node18-macos-arm64"
    ],
    "outputPath": "dist"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}